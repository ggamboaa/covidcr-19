{
	"name": "covid19",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"start": "node server.js"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"core-js": "^3.6.4",
		"express": "^4.17.1",
		"moment": "^2.24.0",
		"server-static": "^2.0.5",
		"vue-graph": "^0.8.7"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.2.0",
		"@vue/cli-plugin-eslint": "^4.2.0",
		"@vue/cli-service": "^4.2.0",
		"babel-eslint": "^10.1.0",
		"bootstrap": "^4.4.1",
		"bootstrap-vue": "^2.9.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-vue": "^6.2.2",
		"prettier": "^2.0.2",
		"prettier-stylelint": "^0.4.2",
		"serve-static": "^1.14.1",
		"stylelint-config-prettier": "^8.0.1",
		"stylelint-prettier": "^1.1.2",
		"vue": "^2.6.11",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/recommended",
			"eslint:recommended",
			"prettier/vue",
			"plugin:prettier/recommended"
		],
		"rules": {
			"linebreak-style": [
				"error",
				"unix"
			],
			"vue/prop-name-casing": [
				"off"
			],
			"vue/no-boolean-default": [
				"error",
				"default-false"
			],
			"vue/no-restricted-syntax": [
				"error"
			],
			"vue/order-in-components": [
				"error",
				{
					"order": [
						"comments",
						"components",
						"computed",
						"delimiters",
						"directives",
						"el",
						"extends",
						"filters",
						"functional",
						"inheritAttrs",
						"methods",
						"mixins",
						"model",
						"name",
						"parent",
						"props",
						"propsData",
						"watch",
						"LIFECYCLE_HOOKS",
						"data",
						"render",
						"renderError",
						"template"
					]
				}
			],
			"vue/require-direct-export": [
				"error"
			],
			"vue/v-on-function-call": [
				"error"
			],
			"vue/valid-v-on": [
				"error",
				{
					"modifiers": [
						"listbox"
					]
				}
			],
			"no-console": [
				"off"
			]
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
